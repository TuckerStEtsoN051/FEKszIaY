TARGETS := operator
	go generate -v ./...

	mkdir _output
	@for target in $(TARGETS); do                                       \
			--ldflags "$(LDFLAGS)"                                      \
			-o ./_output/$${target}                                     \
	mkdir _output
	@for target in $(TARGETS); do                                            \
			-w /go/src/$(ROOT)                                               \
			-v $(PWD):/go/src/$(ROOT)                                        \
				./cmd/$${target};                                            \
			-f $(PWD)/build/$${target}/Dockerfile .;                         \

push:
	@for target in $(TARGETS); do                                       \
	done
	@for target in $(TARGETS); do                      \
			VERSION=$(VERSION) envsubst |              \
	done
